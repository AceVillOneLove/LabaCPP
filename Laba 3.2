#include <iostream>
#include <string>

using namespace std;
void students(string name[], int n, int form[], int age[], int marks[], int k) {
    
    for (int i = 0; i < n; i++) {
        cout << "Enter student " << i+1 << " name\n";
        cin >> name[i];
        cout << "Enter student " << i+1 << "  age\n";
        cin >> age[i];
        cout << "Enter student " << i+1 << "  form\n";
        cin >> form[i];
        cout << "Enter student " << i+1 << "  marks\n";
        cin >> marks[i];
    }
}
void gorime(string name[], int n, int form[], int age[], int marks[], int k, int counter) {
    for (int i = 0; i < n; i++)
        if (form[i] == k && marks[i] >= 9)
        {
            counter ++;
        }
    cout << "The number of studens with good marks are " << counter << endl;
}

    


int main() {
    int k, n, counter = 0;
    printf("Enter number of students: ");
    cin >> n;
    cout << "Enter student course: ";
    cin >> k;
    string *name = new string[n];
    int *form = new int[n];
    int *age = new int[n];
    int *marks = new int[n];
    students(name, n, form, age, marks, k);
    gorime(name, n, form, age, marks, k, counter);
    
    
}
